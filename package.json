{
  "name": "electron-portfolio",
  "version": "1.0.0",
  "description": "Desktop Portfolio Application",
  "main": "src/index.js",
  "scripts": {
  "start": "./node_modules/.bin/electron . --no-sandbox",
  "dev": "webpack --watch & ./node_modules/.bin/electron . --no-sandbox",
  "build": "webpack && electron-builder",
  "build:webpack": "webpack"
},
  "build": {
    "appId": "com.yourname.portfolio",
    "productName": "Portfolio",
    "directories": {
      "output": "dist"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "dependencies": {
    "framer-motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "autoprefixer": "^10.4.22",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "^39.1.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.4",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.18",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  }
}
